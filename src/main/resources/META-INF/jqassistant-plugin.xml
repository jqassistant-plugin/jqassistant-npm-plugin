<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://schema.jqassistant.org/plugin/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant NPM Plugin" id="jqa.plugin.npm"
                    version="${project.version}">
    <description>Provides scanners and rules for NPM based projects.</description>
    <model>
        <class>org.jqassistant.plugin.npm.api.model.PackageDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.BugTrackerDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.PersonDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.FundingDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.BinaryDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.ManDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.RepositoryDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.ScriptDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.ConfigDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.DependencyDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.EngineDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.OsDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.CpuDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.DevEngineDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.ExportDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.OverridesDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.PublishConfigDescriptor</class>
        <class>org.jqassistant.plugin.npm.api.model.WorkspaceDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.npm.impl.PackageJsonScannerPlugin</class>
    </scanner>
    <rules>
        <resource>npm.xml</resource>
    </rules>
</jqassistant-plugin>
